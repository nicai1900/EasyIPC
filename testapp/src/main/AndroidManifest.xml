<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		  package="com.sensorberg.easyipc.testapp">

	<application
		android:name="com.sensorberg.easyipc.testapp.App"
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name="com.sensorberg.easyipc.testapp.MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name="com.sensorberg.easyipc.testapp.EasyIpcActivity"/>
		<activity android:name="com.sensorberg.easyipc.testapp.EasyIpcArchActivity"/>

		<service
			android:process=":service"
			android:name="com.sensorberg.easyipc.testapp.MainService"
			android:enabled="true"
			android:exported="false">
		</service>
	</application>

</manifest>